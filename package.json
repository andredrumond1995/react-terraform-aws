{
  "name": "react-terraform-aws",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 4002",
    "build": "NODE_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "prebuild": "node scripts/prebuild-env.js",
    "postbuild": "node scripts/postbuild-env.js",
    "deploy:prod": "terraform -chdir=terraform apply -auto-approve && npm run build:publish:s3 && sleep 5 && npm run build:publish:s3",
    "build:publish:s3": "npm run build && aws s3 sync ./out s3://my-nextjs-todos-bucket/ --delete",
    "redeploy:prod": "terraform -chdir=terraform destroy -auto-approve && npm run deploy:prod"
  },
  "dependencies": {
    "dotenv": "17.2.1",
    "next": "15.4.4",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3",
    "@tailwindcss/postcss": "4",
    "@types/node": "20",
    "@types/react": "19",
    "@types/react-dom": "19",
    "eslint": "9",
    "eslint-config-next": "15.4.4",
    "tailwindcss": "4",
    "typescript": "5"
  }
}
